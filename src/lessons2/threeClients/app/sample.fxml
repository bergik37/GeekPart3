<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="lessons2.threeClients.app.Controller">
    <children>
        <Button fx:id="send_Button" layoutX="661.0" layoutY="549.0" mnemonicParsing="false" onAction="#send"
                prefHeight="39.0" prefWidth="129.0" text="Send">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <TextField fx:id="write_text" layoutX="15.0" layoutY="550.0" prefHeight="39.0" prefWidth="640.0"
                   promptText="your message"/>
        <TextArea fx:id="output_text" layoutX="15.0" layoutY="14.0" prefHeight="532.0" prefWidth="775.0"/>
    </children>
</Pane>
